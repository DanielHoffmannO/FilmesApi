{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "App": {
    "Alias": "credito-teste-ab"
  },
  "Havan": {
    "UserTokens": {
    }
  },
  "ConnectionStrings": {
    "FilmeConnection": "Data Source=HOFFNOTE;Database=Filme;User ID=Filmes;Password=Filmes"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:dd/MM/yyyy HH:mm:ss}|{Level}|{Solicitacao}]:{Message:lj}{NewLine}{Exception}"
        }
      }
    ]
  }
}